# Environment Variables
# Copy this file to .env.local and fill in your values

# Avalanche Fuji Testnet RPC
FUJI_RPC_URL=https://api.avax-test.network/

# Your deployer wallet private key (DO NOT COMMIT!)
PRIVATE_KEY=your_private_key_here

# Snowtrace API Key for contract verification (Optional, only needed if you want to verify contracts on Snowtrace)
SNOWTRACE_API_KEY=your_snowtrace_api_key

# Deployed contract addresses (filled after deployment)
NEXT_PUBLIC_MARKET_FACTORY_ADDRESS=
NEXT_PUBLIC_PREDICTION_MARKET_ADDRESS=

# Chain config
NEXT_PUBLIC_CHAIN_ID=43113
NEXT_PUBLIC_RPC_URL=https://api.avax-test.network/
NEXT_PUBLIC_EXPLORER_URL=https://testnet.snowtrace.io
