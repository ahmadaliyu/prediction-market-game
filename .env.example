# Environment Variables
# Copy this file to .env.local and fill in your values

# Deployed contract addresses (filled after deployment)
NEXT_PUBLIC_PREDICTION_MARKET_ADDRESS=
NEXT_PUBLIC_MARKET_FACTORY_ADDRESS=

# Avalanche Fuji Testnet RPC (for Hardhat deployment)
FUJI_RPC_URL=https://avalanche-fuji-c-chain-rpc.publicnode.com

# Your deployer wallet private key (DO NOT COMMIT!)
PRIVATE_KEY=your_private_key_here

# Snowtrace API Key for contract verification (optional)
SNOWTRACE_API_KEY=

# OpenAI API Key for AI features
OPENAI_API_KEY=
